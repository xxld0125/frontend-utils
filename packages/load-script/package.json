{
  "name": "@frontendUtils/load-script",
  "version": "0.0.1",
  "description": "基于沙箱隔离技术的UMD脚本加载器，提供安全的远程JavaScript脚本动态加载和执行能力",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.12.1"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "build": "rollup -c",
    "dev": "rollup -c --watch"
  },
  "keywords": [
    "umd",
    "script-loader",
    "dynamic-import",
    "remote-script"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxld0125/frontend-utils.git",
    "directory": "packages/load-script"
  },
  "author": "xxld0125",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "proxy-polyfill": "^0.3.2",
    "xhr": "^2.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
